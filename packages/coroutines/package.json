{
    "name": "@jchptf/coroutines",
    "version": "0.0.23",
    "description": "TODO",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.ts",
    "typesVersions": {
        ">=3.2": {
            "dist/utility": [
                "dist/ts3.1/*"
            ]
        },
        ">=3.1": {
            "dist/utility": [
                "dist/ts3.1/*"
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jheinnic/portfolio-monorepo",
        "homepage": "https://github.com/jheinnic/portfolio-monorepo/tree/master/packages/coroutines"
    },
    "author": "John Heinnickel <jheinnic@hotmail.com>",
    "license": "MIT",
    "keywords": [
        "es6",
        "typescript"
    ],
    "engines": {
        "node": ">=10.13.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "http://localhost:4873"
    },
    "scripts": {
        "build": "yarn run clean && tsc -b src && echo `pwd`",
        "clean": "tsc -b --clean && rm -rf dist build .nyc_output coverage doc && mkdir dist && mkdir build",
        "cover": "yarn test && nyc report --reporter=lcov",
        "doc": "node_modules/.bin/typedoc --mode file --module commonjs --target es6 --tsconfig tsconfig.json --externalPattern node_modules --excludeExternals --excludePrivate --out doc --gitRevision feature_bootstrap src typings",
        "pub": "yarn run build && lerna publish --registry http://localhost:4873",
        "test": "tsc -b test --clean && rm -rf build && mkdir build && tsc -b test && nyc mocha `find build/test -type f -name '*.js'`"
    },
    "devDependencies": {
        "@nestjs/testing": "^5.5.0",
