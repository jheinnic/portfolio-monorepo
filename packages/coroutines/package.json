        "@thi.ng/errors": "^0.1.12",
        "@tyriar/fibonacci-heap": "^2.0.9",
        "ix": "^2.3.5",
        "medium": "^1.0.2",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^6.3.3",
        "simplytyped": "^1.6.0",
        "transducers-js": "^0.4.174",
        "tslib": "^1.9.3"
    }
