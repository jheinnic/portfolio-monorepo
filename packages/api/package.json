{
    "name": "@jchptf/api",
    "version": "0.0.10",
    "description": "Common, generic types & interfaces for jchptf projects",
    "main": "./dist/index.js",
    "typings": "./dist/index.d.ts",
    "repository": "https://github.com/jheinnic/portfolio-monorepo",
    "author": "John Heinnickel <jheinnic@hotmail.com>",
    "license": "Apache-2.0",
    "scripts": {
        "build": "yarn clean && tsc --declaration",
        "clean": "rm -rf *.js *.d.ts .nyc_output build coverage doc decorators mixins",
        "cover": "yarn test && nyc report --reporter=lcov",
        "doc": "node_modules/.bin/typedoc --mode modules --out doc src typings",
        "pub": "yarn build && yarn publish --access public",
        "test": "rm -rf build && tsc -p test && nyc mocha build/test/*.js"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.11.4",
        "mocha": "^5.2.0",
        "nyc": "^12.0.2",
        "simplytyped": "^1.5.2",
        "typedoc": "^0.13.0",
        "typescript": "^3.1.1"
    },
    "engines": {
        "node": ">=8.9.0"
    },
    "dependencies": {
        "@thi.ng/errors": "^0.1.3",
        "parcel-bundler": "^1.10.1",
        "terser": "^3.9.3",
        "tslib": "^1.9.3",
        "typescript": "^3.1.1"
    },
    "keywords": [
        "compare",
        "equality",
        "ES6",
        "generic",
        "interfaces",
        "mixins",
        "type declarations",
        "typescript"
    ],
    "publishConfig": {
        "access": "public"
    }
}
