{
    "name": "@jchptf/api",
    "version": "0.0.15",
    "description": "Common, generic types & interfaces for jchptf projects",
    "main": "./dist/index.js",
    "typings": "./dist/index.d.ts",
    "repository": "https://github.com/jheinnic/portfolio-monorepo",
    "author": "John Heinnickel <jheinnic@hotmail.com>",
    "license": "Apache-2.0",
    "scripts": {
        "build": "yarn clean && tsc --declaration",
        "clean": "rm -rf dist/*.js dist/*.d.ts build .nyc_output build coverage doc; mkdir build",
        "cover": "yarn test && nyc report --reporter=lcov",
        "doc": "node_modules/.bin/typedoc --mode modules --out doc src typings",
        "pub": "yarn build && yarn publish --access public",
        "test": "rm -rf build && tsc -p test && nyc mocha build/test/*.js"
    },
    "devDependencies": {
        "@types/chai": "^4.1.6",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.11.4",
        "@types/sinon": "^5.0.5",
        "chai": "^4.2.0",
        "mocha": "^5.2.0",
        "nyc": "^12.0.2",
        "simplytyped": "^1.5.2",
        "sinon": "^7.0.0",
        "ts-sinon": "^1.0.12",
        "typedoc": "^0.13.0",
        "typescript": "^3.1.1"
    },
    "engines": {
        "node": ">=8.9.0"
    },
    "dependencies": {
        "@loopback/metadata": "^1.0.0",
        "@thi.ng/errors": "^0.1.3",
        "inversify": "^5.0.1",
        "tslib": "^1.9.3",
        "typescript": "^3.1.1"
    },
    "keywords": [
        "compare",
        "equality",
        "ES6",
        "generic",
        "interfaces",
        "mixins",
        "type declarations",
        "typescript"
    ],
    "publishConfig": {
        "access": "public"
    }
}
