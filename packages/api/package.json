{
    "name": "@jchptf/api",
    "version": "0.0.26",
    "description": "Common, generic types & interfaces for jchptf projects",
    "main": "./dist/index.js",
    "typings": "./dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/jheinnic/portfolio-monorepo"
    },
    "author": "John Heinnickel <jheinnic@hotmail.com>",
    "license": "Apache-2.0",
    "scripts": {
        "build": "yarn run clean && tsc --declaration && echo `pwd`",
        "clean": "rm -rf dist build .nyc_output coverage doc; mkdir dist; mkdir build",
        "cover": "yarn test && nyc report --reporter=lcov",
        "doc": "node_modules/.bin/typedoc --mode file --module commonjs --target es6 --tsconfig tsconfig.json --externalPattern node_modules --excludeExternals --excludePrivate --out doc --gitRevision feature_bootstrap src typings",
        "pub": "yarn build && lerna publish --registry http://localhost:4873",
        "test": "rm -rf build && tsc -p test && nyc mocha build/test/*.js"
    },
    "devDependencies": {
        "@jchptf/unit-testing": "^0.0.5",
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.12.18",
        "@types/sinon": "^7.0.2",
        "@types/sinon-chai": "^3.2.2",
        "chai": "^4.2.0",
        "mocha": "^5.2.0",
        "mocha-typescript": "^1.1.17",
        "nyc": "^12.0.2",
        "simplytyped": "^1.5.2",
        "sinon": "^7.0.0",
        "sinon-chai": "^3.3.0",
        "ts-sinon": "^1.0.12",
        "typedoc": "^0.13.0",
        "typescript": "^3.1.4"
    },
    "engines": {
        "node": ">=8.9.0"
    },
    "dependencies": {
        "@jchptf/reflection": "^0.0.6",
        "@jchptf/tupletypes": "^0.0.13",
        "@loopback/metadata": "^1.0.1",
        "@thi.ng/errors": "^0.1.12",
        "reflect-metadata": "^0.1.12",
        "tslib": "^1.9.3"
    },
    "keywords": [
        "compare",
        "equality",
        "ES6",
        "generic",
        "interfaces",
        "mixins",
        "type declarations",
        "typescript"
    ],
    "publishConfig": {
        "access": "public"
    }
}
