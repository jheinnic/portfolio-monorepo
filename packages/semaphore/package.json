{
    "name": "@jchptf/semaphore",
    "version": "0.0.7",
    "description": "TODO",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.ts",
    "typesVersions": {
        ">=3.2": {
            "dist/utility": [
                "dist/ts3.1/*"
            ]
        },
        ">=3.1": {
            "dist/utility": [
                "dist/ts3.1/*"
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jheinnic/portfolio-monorepo",
        "homepage": "https://github.com/jheinnic/portfolio-monorepo/tree/master/packages/semaphore"
    },
    "author": "John Heinnickel <jheinnic@hotmail.com>",
    "license": "MIT",
    "keywords": [
        "es6",
        "typescript"
    ],
    "engines": {
        "node": ">=8.9.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "yarn run clean && tsc --declaration && echo `pwd`",
        "clean": "rm -rf dist build .nyc_output coverage doc; mkdir dist; mkdir build",
        "cover": "yarn test && nyc report --reporter=lcov",
        "doc": "node_modules/.bin/typedoc --mode file --module commonjs --target es6 --tsconfig tsconfig.json --externalPattern node_modules --excludeExternals --excludePrivate --out doc --gitRevision feature_bootstrap src typings",
        "pub": "yarn run build && yarn publish --access public",
        "test": "rm -rf build && mkdir build && tsc -p test && nyc mocha build/test/*.js"
    },
    "devDependencies": {
        "@types/chai": "^4.1.6",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.11.4",
        "@types/sinon": "^5.0.5",
        "chai": "^4.2.0",
        "mocha": "^5.2.0",
        "nyc": "^12.0.2",
        "sinon": "^7.0.0",
        "ts-sinon": "^1.0.12",
        "typedoc": "^0.13.0",
        "typescript": "^3.1.3"
    },
    "dependencies": {
        "@jchptf/api": "^0.0.28",
        "@jchptf/coroutines": "^0.0.21",
        "@jchptf/reflection": "^0.0.7",
        "@jchptf/txtypes": "^0.0.3",
        "@jchptf/unit-testing": "^0.0.7",
        "@nestjs/common": "^5.5.0",
        "medium": "^1.0.2",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^6.3.3",
        "simplytyped": "^1.6.0",
        "tslib": "^1.9.3"
    }
}
