{
    "name": "@jchptf/semaphore",
    "version": "0.0.18",
    "description": "TODO",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.ts",
    "typesVersions": {
        ">=3.3": {
            "dist": [
                "dist/ts3.3/*"
            ]
        },
        ">=3.2": {
            "dist": [
                "dist/ts3.2/*"
            ]
        },
        ">=3.1": {
            "dist": [
                "dist/ts3.1/*"
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jheinnic/portfolio-monorepo",
        "homepage": "https://github.com/jheinnic/portfolio-monorepo/tree/master/packages/semaphore"
    },
    "author": "John Heinnickel <jheinnic@hotmail.com>",
    "license": "MIT",
    "keywords": [
        "es6",
        "typescript"
    ],
    "engines": {
        "node": ">=8.9.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "yarn run clean && tsc -b src && echo `pwd`",
        "clean": "tsc -b --clean && rm -rf dist build .nyc_output coverage doc && mkdir dist && mkdir build",
        "cover": "yarn test && nyc report --reporter=lcov",
        "doc": "node_modules/.bin/typedoc --mode file --module es2015 --target es6 --tsconfig tsconfig.json --externalPattern node_modules --excludeExternals --excludePrivate --out doc --gitRevision feature_bootstrap src typings",
        "pub": "yarn build && lerna publish --registry http://localhost:4873",
        "test": "tsc -b test --clean && rm -rf build && mkdir build && tsc -b test && nyc mocha `find build -type f -name '*.js'`"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.12.18",
        "@types/sinon": "^7.0.2",
        "@types/sinon-chai": "^3.2.2",
        "chai": "^4.2.0",
        "mocha": "^5.2.0",
        "nyc": "^12.0.2",
        "sinon": "^7.2.2",
        "sinon-chai": "^3.3.0",
        "ts-sinon": "^1.0.12",
        "typedoc": "^0.14.2",
        "typescript": "3.2.2"
    },
    "dependencies": {
        "@jchptf/api": "^0.0.37",
        "@jchptf/coroutines": "^0.0.32",
        "@jchptf/nestjs": "^0.0.3",
        "@jchptf/reflection": "^0.0.9",
        "@jchptf/txtypes": "^0.0.8",
        "@nestjs/common": "^5.5.0",
        "medium": "^1.0.2",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^6.3.3",
        "simplytyped": "^1.6.0",
        "tslib": "^1.9.3"
    }
}
