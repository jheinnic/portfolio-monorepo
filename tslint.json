{
  "extends": "tslint-config-airbnb",
  "rules": {
    "brace-style": [false],
    "one-line": [
      true,
      "check-catch",
      "check-finally",
      "check-else",
      "check-whitespace"
    ],
    "interface-name": [true, "always-prefix"],
    "ter-indent": [
      true,
      3,
      {
        "SwitchCase": 1
      }
    ],
    "variable-name": [true, "allow-leading-underscore"]
  }
}
